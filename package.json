{
  "name": "jack-hudson",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.2.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "npm run update-build && rm -rf node_modules/gh-pages/.cache && gh-pages -d build -b master",
    "lint": "eslint --fix . --ext .js,.ts,.tsx",
    "update-build": "npm run copy-robots && npm run copy-gitignore && npm run copy-sitemap && npm run copy-readme && npm run copy-cname",
    "copy-readme": "cp README.md build/README.md",
    "copy-robots": "cp robots.txt build/robots.txt",
    "copy-gitignore": "cp .gitignore build/.gitignore",
    "copy-sitemap": "cp sitemap.xml build/sitemap.xml",
    "copy-cname": "cp CNAME build/CNAME"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://www.jackhudson.com",
  "devDependencies": {
    "@types/puppeteer": "^1.12.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "2.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-import-resolver-babel-module": "2.1.0",
    "eslint-plugin-flowtype": "^3.10.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest-cli": "^24.8.0",
    "prettier": "1.18.2",
    "puppeteer": "^1.18.0",
    "react-dev-utils": "^9.0.1"
  },
  "eslintIgnore": [
    "build/**"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.{spec.js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/tests/**"
    ]
  }
}
